<div class="events_of_day">
	<%= render partial: "events/event", collection: @events_of_day %>
	<% if btns_massive(count_user_events_in_date(@date),(@number_of_events_day/10-2),10).size > 1%>
		<hr>
		<ul class="list-group list-group-horizontal-md">
			<% btns_massive(count_user_events_in_date(@date),(@number_of_events_day/10-2),10).each do |btn| %>
				<%= link_to btn+1, root_path(date: @date, quantity_day: btn*10 , quantity_today: @number_of_events_today), class:"list-group-item list-group-item-action" %>
			<% end -%>
		</ul>
	<% end -%>
</div>
